{
    "name": "abbeal/use-case-bundle",
    "description": "Use case library for symfony-bundle",
    "version": "1.0.0",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [{
        "name": "FEREY StÃ©phane",
        "email": "stephane.ferey@abbeal.com"
    }],
    "autoload": {
        "psr-4": {
            "Abbeal\\Bundle\\UseCaseBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abbeal\\Bundle\\UseCaseBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.0 || ^8.0",
        "beberlei/assert": "^3.3",
        "ramsey/uuid": "^4.1",
        "symfony/dependency-injection": "^4.0|^5.0",
        "symfony/http-foundation": "^4.0|^5.0",
        "symfony/http-kernel": "^4.0|^5.0"
    },
    "require-dev": {
        "composer/semver": "^3.0@dev",
        "friendsofphp/php-cs-fixer": "^2.8",
        "symfony/phpunit-bridge": "^4.3|^5.0",
        "phpstan/phpstan": "*",
        "phpunit/phpunit": ">=6.0.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
